@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf:     <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix qt:     <http://www.w3.org/2001/sw/DataAccess/tests/test-query#> .

<>  rdf:type mf:Manifest ;
    rdfs:label "Sorting" ;
    mf:entries
    ( 
      [  mf:name    "sort-1" ;
         rdfs:label "Alphabetic sort (ascending) on untyped literals" ;
         mf:action
            [ qt:query  <query-sort-1.rq> ;
              qt:data   <data-sort-1.ttl> ] ;
         mf:result  <result-sort-1.rdf>
      ]
      [  mf:name    "sort-2" ;
         rdfs:label "Alphabetic sort (descending) on untyped literals" ;
         mf:action
            [ qt:query  <query-sort-2.rq> ;
              qt:data   <data-sort-1.ttl> ] ;
         mf:result  <result-sort-2.rdf>
      ]
      [  mf:name    "sort-3" ;
         rdfs:label "Sort on (possibly unbound) URIs" ;
         mf:action
            [ qt:query  <query-sort-3.rq> ;
              qt:data   <data-sort-3.ttl> ] ;
         mf:result  <result-sort-3.rdf>
      ]
      [  mf:name    "sort-4" ;
         rdfs:label "Sort on datatyped (integer) literals" ;
         mf:action
            [ qt:query  <query-sort-4.rq> ;
              qt:data   <data-sort-4.ttl> ] ;
         mf:result  <result-sort-4.rdf>
      ]
      [  mf:name    "sort-5" ;
         rdfs:label "Sort first on untyped literals (ascending), then on
	 datatyped (integer) literals (descending" ;
         mf:action
            [ qt:query  <query-sort-5.rq> ;
              qt:data   <data-sort-4.ttl> ] ;
         mf:result  <result-sort-5.rdf>
      ]
      [  mf:name    "sort-6" ;
         rdfs:label "Sort on mixed result of uris and literals." ;
         mf:action
            [ qt:query  <query-sort-6.rq> ;
              qt:data   <data-sort-6.ttl> ] ;
         mf:result  <result-sort-6.rdf>
      ]
      [  mf:name    "sort-7" ;
         rdfs:label "Sort on mixed typed literals (integer and float)" ;
         mf:action
            [ qt:query  <query-sort-4.rq> ;
              qt:data   <data-sort-7.ttl> ] ;
         mf:result  <result-sort-7.rdf>
      ]
    ).
