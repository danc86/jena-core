<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 
  xmlns:rdf      ="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs     ="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:daml     ="http://www.daml.org/2001/03/daml+oil#"
  xmlns:calendar ="http://daml.umbc.edu/ontologies/calendar-ont#"
  xmlns:ittalks  ="http://daml.umbc.edu/ontologies/talk-ont#" 
  xmlns:place    ="http://daml.umbc.edu/ontologies/place-ont#"
  xmlns          ="http://daml.umbc.edu/ontologies/profile-ont#"
>

  <daml:Ontology rdf:about="">
    <daml:versionInfo>$Revision$, $Date$</daml:versionInfo>
  </daml:Ontology>
  
  <daml:Class rdf:ID="Date">
    <daml:equivalentTo>http://daml.umbc.edu/ontologies/calendar#Date</daml:equivalentTo>
  </daml:Class>
  <daml:Class rdf:ID="DayOfWeek">
    <daml:equivalentTo>http://daml.umbc.edu/ontologies/calendar#DayOfWeek</daml:equivalentTo>
  </daml:Class>
  <daml:Class rdf:ID="String">
    <daml:equivalentTo>http://www.daml.org/2001/03/daml+oil#Literal</daml:equivalentTo>
  </daml:Class>
  <daml:Class rdf:ID="Place">
    <daml:equivalentTo>http://daml.umbc.edu/ontologies/place-ont#Place</daml:equivalentTo>
  </daml:Class>

  <!-- Some basic classes for scheduling -->
  <daml:Class rdf:ID="BusyDateTime">
    <rdfs:label>Busy Date and Time</rdfs:label> 
    <daml:subClassOf rdf:resource="#Date"/>
  </daml:Class>
  <rdf:Property rdf:ID="StartDateTime" rdfs:domain="#BusyTime" rdfs:range="#Date"/>
  <rdf:Property rdf:ID="EndtDateTime" rdfs:domain="#BusyTime" rdfs:range="#Date"/>
  
  <daml:Class rdf:ID="Schedule">
    <rdfs:label>Schedule</rdfs:label> 
    <daml:subClassOf rdf:resource="http://www.daml.org/2001/03/daml+oil#Thing"/>
  </daml:Class>
  <rdf:Property rdf:ID="MyBusyDay" rdfs:domain="#Schedule" rdfs:range="#DayOfWeek"/>
  <rdf:Property rdf:ID="MyBusyDateTime" rdfs:domain="#Schedule" rdfs:range="#BusyDateTime"/>
  <!-- end of scheduling -->
  
  
  <!-- Class for filtering -->
  <daml:Class rdf:ID="FilterOn">
    <daml:label>Filter talks based on</daml:label>
    <!-- something for enumeration of location, schedule, interest -->
    <daml:range rdf:resource="http://www.daml.org/2001/03/daml+oil#List" />
  </daml:Class>
  <!-- end of filtering -->
  
  <!-- Classes for Assertions -->
  <daml:Class rdf:ID="RelatedTo">
    <rdfs:label>Related To</rdfs:label>
    <rdfs:comment>Used to assert topics that the user thinks are related</rdfs:comment>
    <daml:subClassOf rdf:resource="http://www.daml.org/2001/03/daml+oil#Thing"/>
  </daml:Class>
  <rdf:Property rdf:ID="Topic1">
    <daml:domain rdf:resource="#RelatedTo"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="Topic2">
    <daml:domain rdf:resource="#RelatedTo"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  
  <daml:Class rdf:ID="SubsumedBy">
    <rdfs:label>Subsumed by</rdfs:label>
    <rdfs:comment>Used to assert topics that the user thinks are related</rdfs:comment>
    <daml:subClassOf rdf:resource="http://www.daml.org/2001/03/daml+oil#Thing"/>
  </daml:Class>
  <rdf:Property rdf:ID="Topic1">
    <daml:domain rdf:resource="#SubsumedBy"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="Topic2">
    <daml:domain rdf:resource="#SubsumedBy"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  
  <daml:Class rdf:ID="Assertions">
    <rdfs:label>Assertions</rdfs:label>
    <rdfs:comment>It is a statement that the user makes to add additional assumptions</rdfs:comment>
  </daml:Class>
  <rdf:Property rdf:ID="RelatedTo">
    <rdfs:label>Related To</rdfs:label>
    <rdfs:domain rdf:resource="#Assertions"/>
    <rdfs:range rdf:resource="#RelatedTo"/>
  </rdf:Property>
  <rdf:Property rdf:ID="SubsumedBy">
    <rdfs:label>Related To</rdfs:label>
    <rdfs:domain rdf:resource="#Assertions"/>
    <rdfs:range rdf:resource="#SubsumedBy"/>
  </rdf:Property>
  
  <!-- end of Assertions-->
  
  
  <!-- Class for Interest -->
  <daml:Class rdf:ID="Interest">
    <rdfs:label>Interested In</rdfs:label>
    <daml:subClassOf rdf:resource="http://www.daml.org/2001/03/daml+oil#Thing"/>
  </daml:Class>
  <rdf:Property rdf:ID="Topic">
    <daml:domain rdf:resource="#Interest"/>
    <daml:range rdf:resource="#String"/>
    <daml:minCardinality>1</daml:minCardinality>
  </rdf:Property>
  <rdf:Property rdf:ID="Classification">
    <daml:domain rdf:resource="#Interest"/>
    <daml:range rdf:resource="#String"/>
    <daml:minCardinality>1</daml:minCardinality>
  </rdf:Property>
  <!-- end of Interest class -->
  
  <!-- Class Profile -->
  <daml:Class rdf:ID="Profile">
    <rdfs:label>Personal Profile</rdfs:label>
    <rdfs:comment>DAML personal profile</rdfs:comment>
    <daml:subClassOf rdf:resource="http://www.daml.org/2001/03/daml+oil#Thing"/>
  </daml:Class>
  <rdf:Property rdf:ID="FName">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
    <daml:minCardinality>1</daml:minCardinality>
  </rdf:Property>
  <rdf:Property rdf:ID="LName">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
    <daml:minCardinality>1</daml:minCardinality>
  </rdf:Property>
  <rdf:Property rdf:ID="Gender">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="Organization">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="Email">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="AgentContact">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="BioSketch">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="OfficeAddress">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#Place"/>
    <daml:minCardinality>1</daml:minCardinality>
  </rdf:Property>
  <rdf:Property rdf:ID="Title">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="PhoneNumber">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="FaxNumber">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="Pager">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
  </rdf:Property>
  <rdf:Property rdf:ID="Interests">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#Interest"/>
  </rdf:Property>
  <rdf:Property rdf:ID="HomeAddress">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#Place"/>
  </rdf:Property>
  <rdf:Property rdf:ID="WebPage-URI">
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#String"/>
    <daml:cardinality>1</daml:cardinality>
  </rdf:Property>
  
  <!-- For this version we only use City as the Location -->
  <rdf:Property rdf:ID="Location">
    <rdfs:comment>I will be at this location most of the time</rdfs:comment>
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#Place"/>
    <daml:cardinality>1</daml:cardinality>
  </rdf:Property>
  <rdf:Property rdf:ID="MySchedule">
    <rdfs:comment>I will be busy during the following days</rdfs:comment>
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#Schedule"/>
  </rdf:Property>
  <rdf:Property rdf:ID="Filter">
    <rdfs:comment>I want the talks filtered by</rdfs:comment>
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#FilterOn"/>
    <daml:minCardinality>3</daml:minCardinality>
  </rdf:Property>
  <rdf:Property rdf:ID="MyAssertions">
    <rdfs:comment>I believe the following assertions</rdfs:comment>
    <daml:domain rdf:resource="#Profile"/>
    <daml:range rdf:resource="#Assertions"/>
  </rdf:Property>
  
  <!-- end of Class Profile -->
  
</rdf:RDF>
