	<!-- *** Latitude Class *** -->
	<daml:Class rdf:ID="Latitude">
		<daml:label>Latitude</daml:label>
		<daml:comment>The angular distance north or south of the earth's equator, measured in degrees along a meridian, as on a map or globe.</daml:comment>
		<!-- *** direction Restrictions *** -->
		<daml:subClassOf>
			<daml:Restriction daml:cardinality="1">
