	<!-- GPS Coordinates Ontology  -->
	<daml:Ontology rdf:about="">
		<ves:versioning>
			<ves:VersionData>
				<dc:title>GPS Coordinates Ontology</dc:title>
				<ves:version>0.1.0</ves:version>
				<ves:email>TeamXML@drc.com</ves:email>
				<dc:creator>DRC</dc:creator>
				<dc:contributor>Dan King</dc:contributor>
				<dc:contributor>Gabriel Aviles</dc:contributor>
				<ves:releaseDate>2002-10-11</ves:releaseDate>
				<ves:releaseLevel>beta</ves:releaseLevel>
				<ves:status>current</ves:status>
			</ves:VersionData>
		</ves:versioning>
		<daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
		<daml:comment>This is an ontology to capture the most popular coordinate systems used by GPS</daml:comment>
	</daml:Ontology>
	<!-- ************************************************************************************************************************** -->
	<!-- *** GPSCoordinates Class  *** -->
	<daml:Class rdf:ID="GPSCoordinates">
		<daml:label>GPS Coordinates</daml:label>
		<!-- *** locationName Restrictions *** -->
		<daml:subClassOf>
			<daml:Restriction daml:maxCardinality="1">
				<daml:onProperty rdf:resource="#locationName"/>
			</daml:Restriction>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction>
				<daml:onProperty rdf:resource="#locationName"/>
				<daml:toClass rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
			</daml:Restriction>
		</daml:subClassOf>
		<!-- *** elevation Restrictions *** -->
		<daml:subClassOf>
			<daml:Restriction daml:maxCardinality="1">
				<daml:onProperty rdf:resource="#elevation"/>
			</daml:Restriction>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction>
				<daml:onProperty rdf:resource="#elevation"/>
				<daml:toClass rdf:resource="#Elevation"/>
			</daml:Restriction>
		</daml:subClassOf>
	</daml:Class>
	<!-- ************************************************************************************************************************** -->
	<!-- *** Degrees Minutes Seconds (DMS) Coordinates Class  *** -->
	<daml:Class rdf:ID="DMSCoordinates">
		<daml:label>Degrees Minutes Seconds (DMS) Coordinates</daml:label>
		<daml:subClassOf rdf:resource="#GPSCoordinates"/>
		<!-- *** latitude Restrictions *** -->
		<daml:subClassOf>
			<daml:Restriction daml:cardinality="1">
				<daml:onProperty rdf:resource="#latitude"/>
			</daml:Restriction>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction>
				<daml:onProperty rdf:resource="#latitude"/>
