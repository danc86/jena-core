<?xml version="1.0"?>
<rdf:RDF
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
 xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
 xmlns:xsd="http://www.w3.org/2000/10/XMLSchema#" 
 xmlns:dc="http://orlando.drc.com/daml/ontology/DC/3.1/dces-ont#"
 xmlns:ves="http://orlando.drc.com/daml/ontology/VES/3.1/drc-ves-ont#" 
 xmlns:org="http://orlando.drc.com/daml/ontology/Organization/3.1.1/Organization-ont#"
 xmlns="http://orlando.drc.com/daml/ontology/Organization/3.1.1/Organization-ont#"
>
	<!-- Organization Ontology  -->
	<daml:Ontology rdf:about="">
		<dc:title>Organization Ontology</dc:title>
		<ves:previous>http://orlando.drc.com/daml/ontology/Organization/3.1/Organization-ont.daml</ves:previous>
		<ves:location>http://orlando.drc.com/daml/ontology/Organization/3.1.1/Organization-ont.daml</ves:location>
		<ves:version>3.1.1</ves:version>
		<ves:email>TeamXML@drc.com</ves:email>
		<dc:creator>DRC</dc:creator>
		<ves:status>published</ves:status>
		<ves:statusDate>2001-10-12</ves:statusDate>
		<daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/ontology/VES/3.1/drc-ves-ont"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/Ontology/daml-extension/3.1/daml-ext-ont"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/ontology/UniversalProperty/3.1/UniversalProperty-ont"/>
		<daml:comment>This is an ontology for a generic organizational structure.</daml:comment>
	</daml:Ontology>
	<!-- Organization Class -->
	<daml:Class rdf:ID="Organization">
		<daml:comment>An organization is something made up of units with varied functions that contribute to the whole and to collective functions.</daml:comment>
		<daml:comment>The building block of an organization is an organizational unit.  Every "part" of the organization is a unit, including the organization itself.</daml:comment>
	</daml:Class>
	<!-- Global Properties -->
	<daml:DatatypeProperty rdf:ID="mission">
		<daml:domain rdf:resource="#Organization"/>
		<daml:domain rdf:resource="#OrganizationType"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
		<daml:comment>Defines the mission (purpose / function) of both the Organzation and the Type.</daml:comment>
	</daml:DatatypeProperty>
	<!-- Organization Properties -->
	<daml:DatatypeProperty rdf:ID="organizationName">
		<daml:subPropertyOf rdf:resource="http://orlando.drc.com/daml/ontology/UniversalProperty/3.1/UniversalProperty-ont#name"/>
		<daml:domain rdf:resource="#Organization"/>
		<daml:comment>The name of the Organization</daml:comment>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="organizationAlias">
		<daml:subPropertyOf rdf:resource="http://orlando.drc.com/daml/ontology/UniversalProperty/3.1/UniversalProperty-ont#alias"/>
		<daml:domain rdf:resource="#Organization"/>
		<daml:comment>An alternate name for the Organization, possibly an acronym.</daml:comment>
	</daml:DatatypeProperty>
	<daml:ObjectProperty rdf:ID="hasType">
		<daml:domain rdf:resource="#Organization"/>
		<daml:range rdf:resource="#OrganizationType"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="hasMember">
		<daml:domain rdf:resource="#Organization"/>
		<daml:range rdf:resource="#Member"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="hasAddress">
		<daml:label>has Address</daml:label>
		<daml:domain rdf:resource="#Organization"/>
		<daml:range rdf:resource="http://orlando.drc.com/daml/ontology/Locator/3.2/Locator-ont#PostalAddress"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="hasTelephoneNumber">
		<daml:label>has Telephone Number</daml:label>
		<daml:domain rdf:resource="#Organization"/>
		<daml:range rdf:resource="http://orlando.drc.com/daml/ontology/Locator/3.2/Locator-ont#TelephoneNumber"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="hasEmail">
		<daml:label>has Email address</daml:label>
		<daml:domain rdf:resource="#Organization"/>
		<daml:range rdf:resource="http://orlando.drc.com/daml/ontology/Locator/3.2/Locator-ont#Email"/>
	</daml:ObjectProperty>
	<daml:TransitiveProperty rdf:ID="partOf">
		<daml:domain rdf:resource="#Organization"/>
		<daml:range rdf:resource="#Organization"/>
		<daml:comment>The "parent" property of a "child" organization which is part of some "parent" organization.</daml:comment>
	</daml:TransitiveProperty>
	<daml:TransitiveProperty rdf:ID="subOrganization">
		<daml:inverseOf rdf:resource="#partOf"/>
		<daml:domain rdf:resource="#Organization"/>
		<daml:range rdf:resource="#Organization"/>
		<daml:comment>The "child" property of a "parent" organization which has "child" / "sub" organization(s).</daml:comment>
	</daml:TransitiveProperty>
	<!-- Organization Type Class -->
	<daml:Class rdf:ID="OrganizationType">
		<daml:label>Organization Type</daml:label>
		<daml:comment>The type of organization.</daml:comment>
	</daml:Class>
	<!-- Organization Type Properties -->
	<daml:DatatypeProperty rdf:ID="typeName">
		<daml:subPropertyOf rdf:resource="http://orlando.drc.com/daml/ontology/UniversalProperty/3.1/UniversalProperty-ont#name"/>
		<daml:domain rdf:resource="#OrganizationType"/>
	</daml:DatatypeProperty>
	<!-- Member Class -->
	<daml:Class rdf:ID="Member">
		<daml:label>Member</daml:label>
		<daml:subClassOf rdf:resource="http://orlando.drc.com/daml/ontology/Person/3.1/Person-ont#Person"/>
		<daml:comment>A person who has membership in an organization.</daml:comment>
	</daml:Class>
	<!-- Member Properties -->
	<daml:DatatypeProperty rdf:ID="role">
		<daml:domain rdf:resource="#Member"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
		<daml:comment>Defines the role of the Member in an organization.</daml:comment>
	</daml:DatatypeProperty>
</rdf:RDF>
