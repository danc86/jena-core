  <rdfs:range rdf:resource="#Publication"/>
 </daml:ObjectProperty>-->

 
 <!--office properties-->

 <daml:DatatypeProperty rdf:ID="floor">
  <rdfs:domain rdf:resource="#Office"/>
  <rdfs:range rdf:resource="#Literal"/>
 </daml:DatatypeProperty>

 <daml:DatatypeProperty rdf:ID="room">
  <rdfs:domain rdf:resource="#Office"/>
  <rdfs:range rdf:resource="#Literal"/>
 </daml:DatatypeProperty>
 
 <!--teaching property-->

 <daml:ObjectProperty rdf:ID="teaches-course">
  <rdfs:domain rdf:resource="#Member"/>
  <rdfs:range rdf:resource="http://cicho0.tripod.com/cs_Courses_ont#Course"/>
 </daml:ObjectProperty>

</rdf:RDF>