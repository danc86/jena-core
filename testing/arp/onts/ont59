<?xml version="1.0"?>
<rdf:RDF
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
 xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
 xmlns:xsd="http://www.w3.org/2000/10/XMLSchema#"
 xmlns:dc="http://orlando.drc.com/daml/ontology/DC/3.2/dces-ont#"
 xmlns:ves="http://orlando.drc.com/daml/ontology/VES/3.2/drc-ves-ont#"
 xmlns:dext="http://orlando.drc.com/daml/Ontology/daml-extension/3.2/daml-ext-ont#"
 xmlns="http://orlando.drc.com/daml/Ontology/daml-extension/3.2/daml-ext-ont#"
 >
	<!-- Entensions to the DAML spec ontology -->
	<daml:Ontology rdf:about="">
		<ves:versioning>
			<ves:VersionData>
				<dc:title>Artifact ontology</dc:title>
				<ves:previous>http://orlando.drc.com/daml/Ontology/daml-extension/3.1/daml-ext-ont.daml</ves:previous>
				<ves:location>http://orlando.drc.com/daml/Ontology/daml-extension/3.2/daml-ext-ont.daml</ves:location>
				<ves:version>3.2</ves:version>
				<ves:email>TeamXML</ves:email>
				<dc:creator>DRC</dc:creator>
				<ves:statusDate>2002-02-25</ves:statusDate>
				<ves:status>published</ves:status>
			</ves:VersionData>
		</ves:versioning>
		<daml:comment>This ontology defines extentions to the DAML spec ontology.</daml:comment>
		<daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/ontology/DC/3.2/dces-ont"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/ontology/VES/3.2/drc-ves-ont"/>
	</daml:Ontology>
	<!-- Define a class to description class  to replace the daml:Ontology class in an artifact (instance file). -->
	<daml:Class rdf:ID="Artifact">
		<daml:label>Artifact</daml:label>
		<daml:comment>A DAML artifact (DAML-A) is a document that uses the vocabulary defined by one or more DAML ontologies (DAML-O).  DAML-A is also referred to as "instance data" and "content."</daml:comment>
		<daml:subClassOf rdf:resource="http://www.daml.org/2001/03/daml+oil#Ontology"/>
	</daml:Class>
	<!-- Extend the daml:Ontology class to include explicitly definied version data. -->
	<!-- Note: this extension should be sufficient; however the DAML Validator does not apply this restriction. A duplicate of this extension was placed in the drc-ves-ont (v3.2) ontology which the DAML Validator recgonizes. Thus, this extension was left here for correctness. -->
	<rdfs:Class rdf:about="http://www.daml.org/2001/03/daml+oil#Ontology">
		<daml:subClassOf>
			<daml:Restriction daml:maxCardinality="1">
				<daml:onProperty rdf:resource="http://orlando.drc.com/daml/Ontology/ves/3.2/drc-ves-ont#versioning"/>
				<daml:toClass rdf:resource="http://orlando.drc.com/daml/Ontology/ves/3.2/drc-ves-ont#VersionData"/>
			</daml:Restriction>
		</daml:subClassOf>
	</rdfs:Class>
</rdf:RDF>
