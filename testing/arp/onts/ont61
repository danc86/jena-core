		<daml:comment>Fugitive Ontology</daml:comment>
	</daml:Ontology>
	<!-- ======================================== -->
	<!-- Define Person  Class and Property Definitions  	-->
	<!-- ======================================== -->
	<daml:Class rdf:ID="Person">
		<daml:label>Person</daml:label>
		<daml:comment>This class represents a particular person (living or dead) independent of a particular description of the person</daml:comment>
	</daml:Class>
	<daml:DatatypeProperty rdf:ID="name">
		<daml:domain rdf:resource="#Person"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:ObjectProperty rdf:ID="placeOfBirth">
		<daml:domain rdf:resource="#Person"/>
		<daml:range rdf:resource="http://www.daml.org/2001/02/geofile/geofile-ont#Location"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="source">
		<daml:domain rdf:resource="#Person"/>
		<daml:range rdf:resource="http://orlando.drc.com/daml/ontology/Bibliographic/3.1/Bibliographic-ont#Bibliographic"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="describedAs">
		<daml:domain rdf:resource="#Person"/>
		<daml:range rdf:resource="#PersonDescription"/>
	</daml:ObjectProperty>
	<!-- ======================================== -->
	<!-- Define PersonDescription  Class and Property Definitions  	-->
	<!-- ======================================== -->
	<daml:Class rdf:ID="PersonDescription">
		<daml:label>Person</daml:label>
		<daml:comment>This class represents a description of a Person (observable, from a particular perspective, and date/time)</daml:comment>
	</daml:Class>
	<daml:DatatypeProperty rdf:ID="sex">
		<daml:domain rdf:resource="#PersonDescription"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="hair">
		<daml:domain rdf:resource="#PersonDescription"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="eyes">
		<daml:domain rdf:resource="#PersonDescription"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="height">
		<daml:domain rdf:resource="#PersonDescription"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="weight">
