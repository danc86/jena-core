	<daml:ObjectProperty rdf:about="source">
		<daml:domain rdf:resource="#PersonDescription"/>
	</daml:ObjectProperty>
	<!-- ======================================== -->
	<!-- Define Fugitive  Class and Property Definitions  	-->
	<!-- ======================================== -->
	<daml:Class rdf:ID="Fugitive">
		<daml:subClassOf rdf:resource="Person"/>
		<daml:label>Fugitive</daml:label>
		<daml:comment>This class represents a specialization of Person(s) that are fugitives</daml:comment>
	</daml:Class>
	<daml:DatatypeProperty rdf:ID="reason">
		<daml:domain rdf:resource="#Fugitive"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="NCIC">
		<daml:domain rdf:resource="#Fugitive"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="caution">
		<daml:domain rdf:resource="#Fugitive"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<!-- ======================================== -->
	<!-- Define Fugitive Description   Class and Property Definitions  	-->
	<!-- ======================================== -->
	<daml:Class rdf:ID="FugitiveDescription">
		<daml:subClassOf rdf:resource="PersonDescription"/>
		<daml:label>Fugitive Description</daml:label>
		<daml:comment>This class represents a specialization of PersonDescription to associate properties for fugitives</daml:comment>
	</daml:Class>
	<daml:DatatypeProperty rdf:ID="alias">
		<daml:domain rdf:resource="#FugitiveDescription"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="dateOfBirthUsed">
		<daml:domain rdf:resource="#FugitiveDescription"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<!-- ======================================== -->
	<!-- Define Terrorist  Class and Property Definitions  	-->
	<!-- ======================================== -->
	<daml:Class rdf:ID="Terrorist">
		<daml:subClassOf rdf:resource="Fugitive"/>
		<daml:label>Terrorist</daml:label>
		<daml:comment>This class represents a specialization of fugitives that are suspected terrorist</daml:comment>
	</daml:Class>
	<!-- ======================================== -->
	<!-- Define Terrorist  Description   Class and Property Definitions  	-->
	<!-- ======================================== -->
	<daml:Class rdf:ID="TerroristDescription">
		<daml:subClassOf rdf:resource="FugitiveDescription"/>
		<daml:label>Terrorist  Description</daml:label>
		<daml:comment>This class represents a specialization of FugitiveDescription to associate properties for terrorists</daml:comment>
	</daml:Class>
	<daml:DatatypeProperty rdf:ID="language">
		<daml:domain rdf:resource="#TerroristDescription"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
