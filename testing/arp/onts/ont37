	<rdfs:type rdf:resource="&daml;DatatypeProperty"/>
</rdf:Property>
<rdfs:Class rdf:about="Image">
	<rdfs:subClassOf rdf:resource="_anon1"/>
</rdfs:Class>
<rdfs:Class rdf:about="ImageOccurence">
	<rdfs:subClassOf rdf:resource="_anon2"/>
	<rdfs:subClassOf rdf:resource="_anon3"/>
</rdfs:Class>
<rdf:Property rdf:about="OnSlide">
	<rdfs:type rdf:resource="&daml;DatatypeProperty"/>
</rdf:Property>
<daml:Restriction rdf:about="_anon0">
	<daml:onProperty rdf:resource="Displays"/>
	<daml:hasValue rdf:resource="ImageOccurence"/>
</daml:Restriction>
