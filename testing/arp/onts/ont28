    <Property rdf:about="#webAuthorIndex">
        <subPropertyOf rdf:resource="#authorIndex"/>
        <subPropertyOf rdf:resource="#hasWebpage"/>
    </Property>

    <Property rdf:about="#webIssueIndex">
        <subPropertyOf rdf:resource="#hasWebpage"/>
        <subPropertyOf rdf:resource="#issueIndex"/>
    </Property>
