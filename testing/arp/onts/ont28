<rdf:RDF xmlns="http://www.daml.org/2001/03/daml+oil#"
    xmlns:gen="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#"
    xmlns:log="http://www.w3.org/2000/10/swap/log#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:serialxsd="http://mnemosyne.umd.edu/daml/serial1.0.xsd#"
    xmlns:this="#"
    xmlns:xsd="http://www.w3.org/2000/10/XMLSchema#">

    <Ontology rdf:about="#serialOnt">
        <comment>An ontology that defines elements for describing something that can be assigned an ISSN.</comment>
        <imports> resource="http://www.daml.org/2001/03/daml+oil#"</imports>
        <versionInfo>serial-ont, v.1.1</versionInfo>
    </Ontology>

    <Class rdf:about="#Contest">
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#contestPrize"/>
            <minCardinality>1</minCardinality>
        </subClassOf>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#contestWinner"/>
            <minCardinality>1</minCardinality>
        </subClassOf>
        <subClassOf rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#Event"/>
    </Class>

    <Class rdf:about="#DateRange">
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#eventStart"/>
            <cardinality>1</cardinality>
        </subClassOf>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#eventEnd"/>
            <cardinality>1</cardinality>
        </subClassOf>
    </Class>

    <Class rdf:about="#PublicationFormat">
      <label>A publication format for a serial</label>
      <disjointUnionOf rdf:parseType="collection">
	<intersectionOf rdf:parseType="collection">
	      <Restriction>
	           <cardinality>1</cardinality>
		   <onProperty rdf:resource="#width"/>
	      </Restriction>
	      <Restriction>
	           <cardinality>1</cardinality>
		   <onProperty rdf:resource="#height"/>
	      </Restriction>
	      <Restriction>
	           <cardinality>1</cardinality>
		   <onProperty rdf:resource="#binding"/>
	      </Restriction>
        </intersectionOf>
        <Restriction>
	    <minCardinalityQ>1</minCardinalityQ>
	    <onProperty rdf:resource="#uri"/>
	    <hasClassQ rdf:resource="http://www.w3.org/2000/10/XMLSchema#anyURI"/>
        </Restriction>
      </disjointUnionOf>
    </Class>

    <Property rdf:about="#width">
        <label>Width of a publication format</label>
	<domain rdf:resource="#PublicationFormat"/>
    </Property>

    <Property rdf:about="#height">
        <label>Height of a publication format</label>
	<domain rdf:resource="#PublicationFormat"/>
    </Property>

    <Property rdf:about="#binding">
        <label>Binding of a publication format</label>
	<domain rdf:resource="#PublicationFormat"/>
    </Property>

    <Property rdf:about="#uri">
        <label>URI of a publication format</label>
	<domain rdf:resource="#PublicationFormat"/>
    </Property>

    <Class rdf:about="#FundingSource">
        <label>Thing that provides funds to an organization</label>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#fundingAgent"/>
            <cardinality>1</cardinality>
        </subClassOf>
    </Class>

    <Class rdf:about="#Issue">
        <label>Issue of a serial</label>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#issueDate"/>
            <cardinality>1</cardinality>
        </subClassOf>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#issueVolume"/>
            <maxCardinality>1</maxCardinality>
        </subClassOf>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#issueNumber"/>
            <maxCardinality>1</maxCardinality>
        </subClassOf>
    </Class>

    <Class rdf:about="#RepeatingEvent">
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#eventFrequency"/>
            <cardinality>1</cardinality>
        </subClassOf>
        <subClassOf rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#Event"/>
    </Class>

    <Class rdf:about="#Serial">
        <label>Serial that can be assigned an ISSN</label>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#hasISSN"/>
            <hasClass rdf:resource="http://mnemosyne.umd.edu/daml/serial1.0.xsd#issn"/>
        </subClassOf>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#summary"/>
            <maxCardinality>1</maxCardinality>
        </subClassOf>
    </Class>

    <Property rdf:about="#acceptsContent">
        <label>What kind of content is considered for publication</label>
    </Property>

    <Property rdf:about="#acceptsContentFrom">
        <label>From whom content is considered for publication</label>
        <range rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#Agent"/>
    </Property>

    <Property rdf:about="#authorIndex">
        <label>author index available at</label>
    </Property>

    <Property rdf:about="#contestPrize">
        <domain rdf:resource="#Contest"/>
        <label>has a prize of</label>
    </Property>

    <Property rdf:about="#contestWinner">
        <domain rdf:resource="#Contest"/>
        <label>has a person who won</label>
    </Property>

    <Property rdf:about="#eventFrequency">
        <domain rdf:resource="#RepeatingEvent"/>
    </Property>

    <Property rdf:about="#eventOccurs">
        <domain rdf:resource="#RepeatingEvent"/>
        <label>one occurrence of a repeating event is</label>
        <range rdf:resource="#DateRange"/>
    </Property>

    <Property rdf:about="#fundingAgent">
        <domain rdf:resource="#FundingSource"/>
        <label>Agent in charge of funding source</label>
        <range rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#Agent"/>
    </Property>

    <Property rdf:about="#fundingAmount">
        <domain rdf:resource="#FundingSource"/>
        <label>Amount of funding provided</label>
    </Property>

    <Property rdf:about="#fundingDate">
        <domain rdf:resource="#FundingSource"/>
        <label>When the funds are provided</label>
    </Property>

    <Property rdf:about="#hasAlternativeTitle">
        <label>Alternative or unofficial title for a serial</label>
    </Property>

    <Property rdf:about="#hasCanonicalTitle">
        <label>has the canonical title</label>
    </Property>

    <Property rdf:about="#hasHomepage">
        <label>has a home page at</label>
        <subPropertyOf rdf:resource="#hasWebpage"/>
    </Property>

    <Property rdf:about="#hasISSN">
        <label>has numerical ISSN</label>
        <range rdf:resource="http://mnemosyne.umd.edu/daml/serial1.0.xsd#issn"/>
    </Property>

    <Property rdf:about="#hasWebpage">
        <label>has a web page at</label>
        <range rdf:resource="http://www.w3.org/2000/10/XMLSchema#anyURI"/>
    </Property>

    <Property rdf:about="#isPublishedBy">
        <domain rdf:resource="#Serial"/>
        <label>The publishing organization</label>
        <range rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#Organization"/>
    </Property>

    <Property rdf:about="#issueDate">
        <domain rdf:resource="#Issue"/>
        <label>issue is dated</label>
        <range rdf:resource="http://mnemosyne.umd.edu/daml/serial1.0.xsd#truncDate"/>
    </Property>

    <Property rdf:about="#issueIndex">
        <domain rdf:resource="#Issue"/>
        <label>has an issue index at</label>
        <range rdf:resource="http://www.w3.org/2000/10/XMLSchema#anyURI"/>
    </Property>

    <Property rdf:about="#issueNumber">
        <domain rdf:resource="#Issue"/>
        <label>issue number within volume</label>
        <range rdf:resource="http://www.w3.org/2000/10/XMLSchema#int"/>
    </Property>

    <Property rdf:about="#issueVolume">
        <domain rdf:resource="#Issue"/>
        <label>issue is from volume</label>
        <range rdf:resource="http://www.w3.org/2000/10/XMLSchema#int"/>
    </Property>

    <Property rdf:about="#issueWebpage">
        <label>issue web page at</label>
        <subPropertyOf rdf:resource="#hasWebpage"/>
    </Property>

    <Property rdf:about="#position">
        <domain rdf:resource="#staffJob"/>
        <label>holds the position</label>
        <range rdf:resource="#staffOffice"/>
    </Property>

    <Property rdf:about="#publicationFrequency">
        <domain rdf:resource="#Serial"/>
        <label>How often this serial is published</label>
        <range rdf:resource="http://mnemosyne.umd.edu/daml/serial1.0.xsd#frequency"/>
    </Property>

    <Class rdf:about="#staffJob">
        <label>A particular job held for a particular length of time</label>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#position"/>
            <cardinality>1</cardinality>
        </subClassOf>
        <subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#Restriction"/>
            <onProperty rdf:resource="#tenure"/>
            <minCardinality>1</minCardinality>
        </subClassOf>
    </Class>

    <Class rdf:about="#staffOffice">
        <label>A particular office a person can hold.</label>
	<subClassOf rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#Thing"/>
    </Class>

    <Class rdf:about="#staffMember">
        <label>A staff member of a serial</label>
        <subClassOf rdf:resource="http://www.cs.umd.edu/projects/plus/DAML/onts/general1.0.daml#Employee"/>
    </Class>

    <Property rdf:about="#successorTitle">
        <domain rdf:resource="#Serial"/>
        <label>was succeeded by</label>
        <range rdf:resource="#Serial"/>
    </Property>

    <Property rdf:about="#summary">
        <label>summary</label>
        <range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
    </Property>

    <Property rdf:about="#tenure">
        <domain rdf:resource="#staffJob"/>
        <label>held/holds position for dates</label>
        <range rdf:resource="#DateRange"/>
    </Property>

