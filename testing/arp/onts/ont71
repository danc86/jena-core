			<daml:Restriction rdf:about="#r-c1-operationalLevelActivityDefinedBy"/>
		</daml:subClassOf>
		<!-- operations performed by -->
		<daml:comment>The OP task operation may be perfromed by TA and possibly other OP tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByOperationalLevelActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByTacticalLevelActivity"/>
		</daml:subClassOf>
		<!-- supports operations for -->
		<daml:comment>The OP task operation supports SN, ST and possibly other OP tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForStrategicNationalActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForStrategicTheaterActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForOperationalLevelActivity"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- *************** Tactical Level Activity Class **************** -->
	<daml:Class rdf:ID="TacticalLevelActivity">
		<daml:comment>This class represents information about Tactical Level (TA) Activities.</daml:comment>
		<rdfs:subClassOf rdf:resource="#Activity"/>
		<daml:disjointWith rdf:resource="#StrategicNationalLevelActivity"/>
		<daml:disjointWith rdf:resource="#StrategicTheaterLevelActivity"/>
		<daml:disjointWith rdf:resource="#OperationalLevelActivity"/>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-tacticalLevelActivityDefinedBy"/>
		</daml:subClassOf>
		<!-- operations performed by -->
		<daml:comment>The TA task operation may be perfromed by possibly other TA tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByTacticalLevelActivity"/>
		</daml:subClassOf>
		<!-- supports operations for -->
		<daml:comment>The TA task operation supports SN, ST, OP and possibly other TA tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForStrategicNationalActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForStrategicTheaterActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForOperationalLevelActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForTacticalLevelActivity"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- ****************************************** Named Restrictions ************************************** -->
	<daml:Restriction daml:minCardinality="1" rdf:ID="r-n1-highestLevelStartingActivityForList">
		<daml:onProperty rdf:resource="#highestLevelStartingActivityForList"/>
		<daml:toClass rdf:resource="#Activity"/>
	</daml:Restriction>
	<daml:Restriction daml:cardinality="1" rdf:ID="r-c1-stategicNationalLevelActivityDefinedBy">
