	<!-- UJTL Scenario Ontology-->
	<daml:Ontology rdf:about="">
		<ves:versioning>
			<ves:VersionData>
				<dc:title>Universal Joint Task List</dc:title>
				<ves:previous>http://orlando.drc.com/daml/ontology/TaskListUJTLScenario/3.1.1/TaskListUJTLScenario-ont.daml</ves:previous>
				<ves:location>http://orlando.drc.com/daml/ontology/TaskListUJTLScenario/3.2/TaskListUJTLScenario-ont.daml</ves:location>
				<ves:version>3.2</ves:version>
				<ves:email>TeamXML@drc.com</ves:email>
				<dc:creator>DRC</dc:creator>
				<ves:statusDate>2002-02-27</ves:statusDate>
				<ves:status>published</ves:status>
			</ves:VersionData>
		</ves:versioning>
		<daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/Ontology/daml-extension/3.2/daml-ext-ont"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/ontology/TaskList/3.1.2/TaskList-ont"/>
		<daml:comment>This is an ontology to describe a particular scenario or activity list defined from the Universal Joint Task List  (UJTL).   It references the task list ontology, and defines task categories and the relationships between them.</daml:comment>
	</daml:Ontology>
	<!-- *************** Scenario of Universal Joint Task: Classes and Properties **************** -->
	<!-- *************** Strategic Level - National Military Task Classes and Properties **************** -->
	<!-- ActivityLists Class ********************************* -->
	<daml:Class rdf:ID="ActivityLists">
		<daml:label>Activity Lists</daml:label>
		<daml:comment>Starting activity for the Scenario</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-n1-highestLevelStartingActivityForList"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="http://orlando.drc.com/daml/ontology/TaskList/3.1.2/TaskList-ont#r-c1-title"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-x1-description"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- Activity Class ********************************* -->
	<daml:Class rdf:ID="Activity">
		<daml:label>Activity</daml:label>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-x1-description"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="http://orlando.drc.com/daml/ontology/TaskList/3.1.2/TaskList-ont#r-x1-title"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about=""/>
		</daml:subClassOf>
	</daml:Class>
	<!-- *************** Strategic Level - National Level Activity Class  **************** -->
	<daml:Class rdf:ID="StrategicNationalLevelActivity">
		<daml:comment>This class represents information about Strategic National Level (SN) Activities.</daml:comment>
		<rdfs:subClassOf rdf:resource="#Activity"/>
		<daml:disjointWith rdf:resource="#StrategicTheaterLevelActivity"/>
		<daml:disjointWith rdf:resource="#OperationalLevelActivity"/>
		<daml:disjointWith rdf:resource="#TacticalLevelActivity"/>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-strategicNationalLevelActivityDefinedBy"/>
		</daml:subClassOf>
		<!-- operations performed by -->
		<daml:comment>The SN task operation is perfromed by ST, OP, TA or possibly other SN tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByStrategicNationalActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByStrategicTheaterActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByOperationalLevelActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByTacticalLevelActivity"/>
		</daml:subClassOf>
		<!-- supports operations for -->
		<daml:comment>The SN task operation supports possibly other SN tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForStrategicNationalActivity"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- *************** Strategic Level - Theater Level Activity Class  **************** -->
	<daml:Class rdf:ID="StrategicTheaterLevelActivity">
		<daml:comment>This class represents information about Strategic Theater Level (ST) Activities.</daml:comment>
		<rdfs:subClassOf rdf:resource="#Activity"/>
		<daml:disjointWith rdf:resource="#StrategicNationalLevelActivity"/>
		<daml:disjointWith rdf:resource="#OperationalLevelActivity"/>
		<daml:disjointWith rdf:resource="#TacticalLevelActivity"/>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-strategicTheaterLevelActivityDefinedBy"/>
		</daml:subClassOf>
		<!-- operations performed by -->
		<daml:comment>The ST task operation is perfromed by OP, TA or possibly other ST tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByStrategicTheaterActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByOperationalLevelActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-operationPerformedByTacticalLevelActivity"/>
		</daml:subClassOf>
		<!-- supports operations for -->
		<daml:comment>The ST task operation supports SN tasks and possibly other ST tasks.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForStrategicNationalActivity"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supportsOperationForStrategicTheaterActivity"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- *************** Operational Level Activity Class **************** -->
	<daml:Class rdf:ID="OperationalLevelActivity">
		<daml:comment>This class represents information about Operational Level (OP) Activities.</daml:comment>
		<rdfs:subClassOf rdf:resource="#Activity"/>
		<daml:disjointWith rdf:resource="#StrategicNationalLevelActivity"/>
