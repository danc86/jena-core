	</daml:Ontology>
	<!-- ================================== -->
	<!-- ==  Reference...Class and Property Definitions  -->
	<!-- ================================== -->
	<daml:Class rdf:ID="Bibliographic">
		<daml:label>Bibliographic</daml:label>
		<daml:comment>includes elements that can be used as data categories in bibliographic citations included in terminology databases</daml:comment>
	</daml:Class>
	<daml:Class rdf:ID="BibliographicEntry">
		<daml:label>Bibliographic Entry</daml:label>
		<daml:comment>An entry whose content consists of a standard bibliographic citation</daml:comment>
		<daml:sameClassAs rdf:resource="#Bibliographic"/>
	</daml:Class>
	<daml:ObjectProperty rdf:ID="sourceFor">
		<daml:label>Source For</daml:label>
		<daml:domain rdf:resource="#Bibliographic"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="referenceFor">
		<daml:samePropertyAs rdf:resource="#sourceFor"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="bibliographicEntryFor">
		<daml:samePropertyAs rdf:resource="#sourceFor"/>
	</daml:ObjectProperty>
	<daml:DatatypeProperty rdf:ID="advertisement">
		<daml:label>Advertisement</daml:label>
		<daml:comment>An advertisement or public notice within a serial publication</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="article">
		<daml:label>Article</daml:label>
		<daml:comment>An independent text forming a part of a publication</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="author">
		<daml:label>Author</daml:label>
		<daml:comment>A person or corporate body responsible for the intellectual or artistic content of a document</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="available">
		<daml:label>Available</daml:label>
		<daml:comment>Commercial source from which a cited document can be obtained or a location where the document can be found</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="book">
		<daml:label>Book</daml:label>
		<daml:comment>Publication complete in one volume or complete or intended to be completed in a finite number of volumes</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="category">
		<daml:label>Category</daml:label>
		<daml:comment>Type of publication</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="publicationCategory">
		<daml:samePropertyAs rdf:resource="#category"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="citation">
		<daml:label>Citation</daml:label>
		<daml:comment>A reference to a book, article, of other source</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="city">
		<daml:label>City</daml:label>
		<daml:comment>City and State in which a document is published</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="coden">
		<daml:label>CODEN</daml:label>
		<daml:comment>A concise, unique, alphanumeric code assigned to serial and monographic publications and used as an unambiguous, permanent identifier</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="codeNumber">
		<daml:samePropertyAs rdf:resource="#coden"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="corporateAuthor">
		<daml:label>Corporate Author</daml:label>
		<daml:comment>The organization responsible for the creation of all or some of the intellectual or artistic content of the work</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="copyright">
		<daml:label>Copyright</daml:label>
		<daml:comment>The exclusive legal right granted for a specified period to an author, designer, etc. or another appointed person, to print, publish, perform, film, or record orignal literary, artistic, or musical material</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="copyrightHolder">
		<daml:label>Copyright Holder</daml:label>
		<daml:comment>The name of the copyright owner at the time of publication</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="country">
		<daml:label>Country</daml:label>
		<daml:comment>Country in which a document is published</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="dateOfPublication">
		<daml:label>Date of Publication</daml:label>
		<daml:comment>Indication of the year, and if necessary, of the month and day of publishing</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="publicationDate">
		<daml:samePropertyAs rdf:resource="#dateOfPublication"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="edition">
		<daml:label>Edition</daml:label>
		<daml:comment>Whole set of copies of a document produced from one composition or from a single copy used as a master</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="editor">
		<daml:label>Editor</daml:label>
		<daml:comment>An organization or person responsible for the preparation for publication of a document from the point of view of its intellectual content</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="extent">
		<daml:label>Extent</daml:label>
		<daml:comment>Number of pages in a work</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="firstName">
		<daml:label>First Name</daml:label>
		<daml:comment>The given name of an individual</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="glossary">
		<daml:label>Glossary</daml:label>
		<daml:comment>Terminilogical list of short dictionary containing the terminology of a specific subject field or of related fields</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="isbn">
		<daml:label>ISBN</daml:label>
		<daml:comment>Internationally recognized unique standard number assigned to each edition of a book or other monographic publication for identification purposes</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="issn">
		<daml:label>ISSN</daml:label>
		<daml:comment>Internationally recognized unique standard number assigned to each edition of a book or other monographic publication for identification purposes</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="issueNumber">
		<daml:label>Issue Number</daml:label>
		<daml:comment>Number of an element of a document published over a period of time</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="libraryOfCongressCardNumber">
		<daml:label>Library Of Congress Card Number</daml:label>
		<daml:comment>Number assigned by the U.S. Library of Congress for the purpose of probiding access to a complete catalog record for a work</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="organizationName">
		<daml:label>Organization Name</daml:label>
		<daml:comment>Name of an association, business or other systematically structured entity</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="otherInformation">
		<daml:label>Other Information</daml:label>
		<daml:comment>Generic data catefory used in ISO 12083 for items not otherwise treated in the list of elements; used in terminology database management for terminological elements not normally treated in library-oriented bibliographical resources</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="page">
		<daml:label>Page</daml:label>
		<daml:comment>One side of a leaf of a document</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="publisherName">
		<daml:label>Publisher Name</daml:label>
		<daml:comment>Name of person or organization responsible for the production and dissemination of a ducument</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="reportIdentifier">
		<daml:label>Report Identifier</daml:label>
		<daml:comment>Complete formatted alphanumeric designation uniquely identifying a report</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="role">
		<daml:label>Role</daml:label>
		<daml:comment>Special function of an originator of a document</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="serialTitle">
		<daml:label>Serial Title</daml:label>
		<daml:comment>The title of a monographic series</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="surname">
		<daml:label>Surname</daml:label>
		<daml:comment>The dereditary name of an individual</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="title">
		<daml:label>Title</daml:label>
		<daml:comment>A word or phrase, usually appearing on the document, by which it is convenient to refer to it, which can be used to identify it, and which often (though not invariably) distinguishes it from any other document</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="volumeIdentifier">
		<daml:label>Volume Identifier</daml:label>
		<daml:comment>Identifier for a material unit assembling a certain number of leaves under one cover to form a whole or part of a set</daml:comment>
		<daml:domain rdf:resource="#Bibliographic"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
