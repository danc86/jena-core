	<!-- ================= -->
	<!-- == Bibliographic Ontology    -->
	<!-- ================= -->
	<daml:Ontology rdf:about="">
		<dc:title>Bibliographic Ontology</dc:title>
		<ves:location>http://orlando.drc.com/daml/Ontology/Bibliographic/3.1/Bibliographic-ont</ves:location>
		<ves:version>3.1</ves:version>
		<ves:email>TeamXML@drc.com</ves:email>
		<dc:creator>DRC</dc:creator>
		<ves:statusDate>2001-09-26</ves:statusDate>
		<ves:status>published</ves:status>
		<daml:comment>A sample Bibliographic ontology, with data types taken from ISO 12620:1999(E) Annex B</daml:comment>
		<daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/ontology/VES/3.1/drc-ves-ont"/>
