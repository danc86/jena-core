	<!-- ======================================== -->
	<!-- Class and Property Definitions  -->
	<!-- ======================================== -->

	<daml:Class rdf:ID="IntelligenceReport">
		<daml:label>Report</daml:label>
		<daml:comment>This class represents an IntelLink Intelligence Report</daml:comment>
	</daml:Class>
	<daml:ObjectProperty rdf:ID="conclusion">
		<daml:domain rdf:resource="#IntelligenceReport"/>
		<daml:range rdf:resource="#Conclusion"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="event">
		<daml:domain rdf:resource="#IntelligenceReport"/>
		<daml:range rdf:resource="#Event"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="metadata">
		<daml:domain rdf:resource="#IntelligenceReport"/>
		<daml:range rdf:resource="#Metadata"/>
	</daml:ObjectProperty>

	<daml:Class rdf:ID="Event">
		<daml:label>Event</daml:label>
		<daml:comment>This class represents a major event that is documented within a Report</daml:comment>
	</daml:Class>
	<daml:DatatypeProperty rdf:ID="analysis">
		<daml:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:ObjectProperty rdf:ID="reporter">
		<daml:domain rdf:resource="#Event"/>
		<daml:range rdf:resource="#Person"/>
	</daml:ObjectProperty >
	<daml:ObjectProperty rdf:ID="activity">
		<daml:domain rdf:resource="#Event"/>
		<daml:range rdf:resource="#Activity"/>
	</daml:ObjectProperty >
	<daml:ObjectProperty rdf:ID="note">
		<daml:domain rdf:resource="#Event"/>
		<daml:range rdf:resource="#Note"/>
	</daml:ObjectProperty >
	<daml:ObjectProperty rdf:ID="predecessor">
		<daml:domain rdf:resource="#Event"/>
		<daml:range rdf:resource="#Event"/>
	</daml:ObjectProperty >
	<daml:DatatypeProperty rdf:ID="eventType">
		<daml:domain rdf:resource="#Event"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	
	<daml:Class rdf:ID="Activity">
		<daml:label>Activity</daml:label>
		<daml:comment>This class represents an Activity that occurs within an Event</daml:comment>
	</daml:Class>
	<daml:ObjectProperty rdf:ID="directObject">
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Person"/>
		<daml:range rdf:resource="#Vehicle"/>
		<daml:range rdf:resource="#Organization"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="indirectObject">
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Person"/>
		<daml:range rdf:resource="#Vehicle"/>
		<daml:range rdf:resource="#Organization"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="subject">
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Person"/>
		<daml:range rdf:resource="#Vehicle"/>
		<daml:range rdf:resource="#Organization"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="source">
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Person"/>
		<daml:range rdf:resource="#Vehicle"/>
		<daml:range rdf:resource="#Organization"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="timeframe">
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Timeframe"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="location">
		<daml:domain rdf:resource="#Activity"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="resultedIn">
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Activity"/>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="willResultIn">
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Activity"/>
	</daml:ObjectProperty>
	<daml:DatatypeProperty rdf:ID="frequency">
		<daml:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="rationale">
		<daml:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="duration">
		<daml:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
		<daml:domain rdf:resource="#Activity"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="verb">
		<daml:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
		<daml:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:DatatypeProperty>


	<daml:Class rdf:ID="Note">
		<daml:label>Note</daml:label>
		<daml:comment>This class is used to associate textual notes with an instance of a class</daml:comment>
	</daml:Class>
	<daml:DatatypeProperty rdf:ID="comment">
