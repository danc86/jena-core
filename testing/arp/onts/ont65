	</daml:DatatypeProperty>
	<daml:ObjectProperty rdf:ID="regarding">
		<daml:domain rdf:resource="#Note"/>
		<daml:range rdf:resource="#Activity"/>
		<daml:range rdf:resource="#Person"/>
		<daml:range rdf:resource="#Timeframe"/>
		<daml:range rdf:resource="#Organization"/>
	</daml:ObjectProperty>

	<daml:Class rdf:ID="Conclusion">
		<daml:label>Conclusion</daml:label>
		<daml:comment>This class represents conlusion of intelligence report analysis</daml:comment>
	</daml:Class>
	<daml:ObjectProperty rdf:ID="conclusionEvent">
		<daml:domain rdf:resource="#Conclusion"/>
		<daml:range rdf:resource="#Event"/>
	</daml:ObjectProperty>


	<daml:Class rdf:ID="Metadata">
