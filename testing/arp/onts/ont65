	<daml:Class rdf:ID="Motorcycle">
		<daml:label>Motorcycle</daml:label>
		<daml:comment>This class represents two wheeled motorized vehicles</daml:comment>
		<daml:subClassOf rdf:resource="#Vehicle"/>
		<daml:disjointWith rdf:resource="#Automobile"/>
	</daml:Class>
	
