>
	<!-- TaskList Ontology          -->
	<daml:Ontology rdf:about="">
		<ves:versioning>
			<ves:VersionData>
				<dc:title>TaskList Ontology</dc:title>
				<ves:previous>http://orlando.drc.com/daml/ontology/TaskList/3.1.1/TaskList-ont.daml</ves:previous>
				<ves:location>http://orlando.drc.com/daml/ontology/TaskList/3.1.2/TaskList-ont.daml</ves:location>
				<ves:version>3.1.2</ves:version>
				<ves:email>TeamXML@drc.com</ves:email>
				<dc:creator>DRC</dc:creator>
				<ves:statusDate>2002-02-27</ves:statusDate>
				<ves:status>published</ves:status>
			</ves:VersionData>
		</ves:versioning>
		<daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
		<daml:imports rdf:resource="http://orlando.drc.com/daml/Ontology/daml-extension/3.2/daml-ext-ont"/>
		<daml:comment>ontology for representing descriptions of tasks and structural hierarchy, not the semantics of tasks.</daml:comment>
	</daml:Ontology>
	<!-- *********************** Classes ********************************* -->
	<!-- TaskList Class ********************************* -->
	<daml:Class rdf:ID="TaskList">
		<daml:label>Task List</daml:label>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-hasTask"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-title"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-primarySource"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supplementalSource"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- Task Class ********************************* -->
	<daml:Class rdf:ID="Task">
		<daml:label>Task</daml:label>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-status"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-definition"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-note"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-u-superTask"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-subTask"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-categoryCode"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-index"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-title"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-primarySource"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supplementalSource"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- Doctrine Class ********************************* -->
	<daml:Class rdf:ID="Doctrine">
		<daml:label>Doctrine"</daml:label>
		<daml:comment>Represents a particular doctrine with a link to a copy on the web.</daml:comment>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-link"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-categoryCode"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-c1-index"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-x1-title"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-primarySource"/>
		</daml:subClassOf>
		<daml:subClassOf>
			<daml:Restriction rdf:about="#r-supplementalSource"/>
		</daml:subClassOf>
	</daml:Class>
	<!-- ****************************************** Named Restrictions ************************************** -->
	<!-- TaskList  Restrictions ********************************** -->
	<daml:Restriction rdf:ID="r-hasTask">
		<daml:onProperty rdf:resource="#hasTask"/>
		<daml:toClass rdf:resource="#Task"/>
	</daml:Restriction>
	<!-- Task Restrictions ********************************** -->
	<daml:Restriction daml:cardinality="1" rdf:ID="r-c1-status">
		<daml:onProperty rdf:resource="#status"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:Restriction>
	<daml:Restriction daml:cardinality="1" rdf:ID="r-c1-definition">
		<daml:onProperty rdf:resource="#definition"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:Restriction>
	<daml:Restriction daml:cardinality="1" rdf:ID="r-c1-note">
		<daml:onProperty rdf:resource="#note"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:Restriction>
	<daml:Restriction rdf:ID="r-u-superTask">
		<daml:onProperty rdf:resource="#superTask"/>
		<daml:toClass rdf:resource="#Task"/>
	</daml:Restriction>
	<daml:Restriction rdf:ID="r-subTask">
		<daml:onProperty rdf:resource="#subTask"/>
		<daml:toClass rdf:resource="#Task"/>
	</daml:Restriction>
	<!-- Doctrine Restrictions ********************************** -->
	<daml:Restriction rdf:ID="r-link">
		<daml:onProperty rdf:resource="#link"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#anyURI"/>
	</daml:Restriction>
	<!-- Shared Restrictions ********************************** -->
	<daml:Restriction daml:cardinality="1" rdf:ID="r-c1-categoryCode">
		<daml:onProperty rdf:resource="#categoryCode"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:Restriction>
	<daml:Restriction daml:cardinality="1" rdf:ID="r-c1-index">
		<daml:onProperty rdf:resource="#index"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:Restriction>
	<daml:Restriction daml:cardinality="1" rdf:ID="r-c1-title">
		<daml:onProperty rdf:resource="#title"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:Restriction>
	<daml:Restriction daml:maxCardinality="1" rdf:ID="r-x1-title">
		<daml:onProperty rdf:resource="#title"/>
		<daml:toClass rdf:resource="http://www.w3.org/2000/10/XMLSchema#string"/>
	</daml:Restriction>
	<daml:Restriction rdf:ID="r-primarySource">
		<daml:onProperty rdf:resource="#primarySource"/>
		<daml:toClass rdf:resource="#Doctrine"/>
	</daml:Restriction>
	<daml:Restriction rdf:ID="r-supplementalSource">
		<daml:onProperty rdf:resource="#supplementalSource"/>
		<daml:toClass rdf:resource="#Doctrine"/>
	</daml:Restriction>
	<!-- ******* Properties ************************************** -->
	<!-- TaskList  Properties ********************************** -->
	<daml:ObjectProperty rdf:ID="hasTask">
		<daml:label>has Task</daml:label>
	</daml:ObjectProperty>
	<!-- Task Properties ********************************** -->
	<daml:DatatypeProperty rdf:ID="status">
		<daml:label>Status</daml:label>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="definition">
		<daml:label>Definition</daml:label>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="note">
		<daml:label>Note</daml:label>
	</daml:DatatypeProperty>
	<daml:TransitiveProperty rdf:ID="superTask">
		<daml:label>Super Task</daml:label>
		<daml:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
	</daml:TransitiveProperty>
	<daml:TransitiveProperty rdf:ID="subTask">
		<daml:label>Sub Task</daml:label>
		<daml:inverseOf rdf:resource="#superTask"/>
	</daml:TransitiveProperty>
	<!-- Doctrine Properties ********************************** -->
	<daml:DatatypeProperty rdf:ID="link">
		<daml:label>Link</daml:label>
	</daml:DatatypeProperty>
	<!-- Shared Properties ********************************** -->
	<daml:DatatypeProperty rdf:ID="categoryCode">
		<daml:label>Category Code</daml:label>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="index">
		<daml:label>Index</daml:label>
	</daml:DatatypeProperty>
	<daml:DatatypeProperty rdf:ID="title">
		<daml:label>Title</daml:label>
	</daml:DatatypeProperty>
	<daml:ObjectProperty rdf:ID="source">
		<daml:label>Source</daml:label>
		<daml:comment>Reference to source.</daml:comment>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="primarySource">
		<daml:label>Primary Source</daml:label>
		<daml:subPropertyOf rdf:resource="#source"/>
		<daml:comment>Reference to primary source.</daml:comment>
	</daml:ObjectProperty>
	<daml:ObjectProperty rdf:ID="supplementalSource">
		<daml:label>Supplemental Source</daml:label>
		<daml:subPropertyOf rdf:resource="#source"/>
		<daml:comment>Reference to a supplemental source.</daml:comment>
	</daml:ObjectProperty>
</rdf:RDF>
