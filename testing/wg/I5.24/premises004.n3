_:a rdf:type owl:Thing .
_:b rdf:type owl:Restriction .
_:b owl:onProperty second:prop .
second:A rdf:type owl:Class .
_:b owl:allValuesFrom second:A .
_:a rdfs:subClassOf _:b .
